<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.bugstore">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Access_module.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StoreActivity" />
        <activity android:name=".Access_module.RegisterActivity" />
        <activity android:name=".CreateProductActivity" />
        <activity android:name=".ProductBuyActivity" />
        <activity android:name=".DataActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".UpdateProfileActivity" />
        <activity android:name=".ForumActivity" />
        <activity android:name=".AddQuestionActivity" />
        <activity android:name=".QAActivity"></activity>
    </application>

</manifest>